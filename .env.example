# Sentinel-Voice Environment Variables
# Copy this file to .env and fill in your API keys

# Gemini API (Main detector)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API (Multi-LLM comparison)
OPENAI_API_KEY=your_openai_api_key_here

# DeepSeek API (Multi-LLM comparison)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Perplexity API (Multi-LLM comparison)
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# ClovaX API (Multi-LLM comparison)
CLOVAX_API_KEY=your_clovax_api_key_here
CLOVAX_API_KEY_PRIMARY=your_clovax_primary_key_here
CLOVAX_REQUEST_ID=your_clovax_request_id_here
